<Window x:Class="PresentationMVVM_WPFCore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PresentationMVVM_WPFCore"
        xmlns:vm="clr-namespace:PresentationMVVM_WPFCore.ViewModels"
 
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" DataContext="{StaticResource Main}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>

        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Content="User" Command="{Binding ToUserCommand}"/>
        <Button Grid.Column="1" Content="Event" Command="{Binding ToEventCommand}"/>

    </Grid>
</Window>
